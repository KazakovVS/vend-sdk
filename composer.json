{
  "name": "simplesquid/vend-sdk",
  "description": "An unoffical PHP SDK for Vend POS",
  "type": "library",
  "keywords": [
    "php",
    "sdk",
    "point-of-sale",
    "vend"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matthew Poulter",
      "email": "matthew.poulter@simplesquid.co.za",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "spatie/data-transfer-object": "^1.8",
    "nesbot/carbon": "^2.23"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0",
    "filp/whoops": "^2.5",
    "rdehnhardt/var-dumper": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "SimpleSquid\\Vend\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
